  id="GBE-delete.dialog"
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  title="&delete.form.title;"
  buttons="accept,cancel"
  buttonlabelaccept="&delete.button-delete.label;"
  buttonlabelcancel="&delete.button-cancel.label;" 
  minheight= "100px" minwidth="400px" flex="1" orient="vertical"
